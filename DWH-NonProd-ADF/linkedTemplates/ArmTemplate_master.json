{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DWH-NonProd-ADF"
		},
		"SRTEST_IR_SH_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SRTEST_IR_SH'"
		},
		"BlobStore_nfdwhdatalake_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "NFDWHNonProdBlobdatalakeConn"
		},
		"BlobStore_nfdwhstaging_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "NFDWHNonProdBlobstagingConn"
		},
		"NFDWHNonProdAKV_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://NFDWHNonProdAKV.vault.azure.net/"
		},
		"SQLdbConnection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "NFDWHDEVSQLConn"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SRTEST_IR_SH_connectionString": {
						"value": "[parameters('SRTEST_IR_SH_connectionString')]"
					},
					"NFDWHNonProdAKV_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('NFDWHNonProdAKV_LS_properties_typeProperties_baseUrl')]"
					},
					"BlobStore_nfdwhdatalake_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('BlobStore_nfdwhdatalake_properties_typeProperties_connectionString_secretName')]"
					},
					"BlobStore_nfdwhstaging_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('BlobStore_nfdwhstaging_properties_typeProperties_connectionString_secretName')]"
					},
					"SQLdbConnection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('SQLdbConnection_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		},
		{
			"name": "ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_2')]"
			]
		},
		{
			"name": "ArmTemplate_4",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_4.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_3')]"
			]
		}
	]
}