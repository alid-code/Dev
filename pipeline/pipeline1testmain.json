{
	"name": "pipeline1testmain",
	"properties": {
		"activities": [
			{
				"name": "SetVarSuccess",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "RunPipelineBudget",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "PipelineName",
					"value": {
						"value": "Master Pipeline Completed",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SendSuccessMail",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "SetVarSuccess",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://prod-24.australiaeast.logic.azure.com:443/workflows/e28a90add06c495bbc9362b5109fb4c0/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=j2wEDAKOpnQ83iKkUa84aeGE6Nfb5604lp-z1-W4ek0",
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"dataFactoryName": "@pipeline().DataFactory",
						"pipelineName": "@pipeline().Pipeline",
						"messageHeader": "***SUCCESS***",
						"messageProcessed": "@variables('PipelineName')",
						"messageNotProcessed": "",
						"messageErrorDuringProcess": ""
					}
				}
			},
			{
				"name": "SendFailMail",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "SetVarBudget",
						"dependencyConditions": [
							"Completed"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://prod-24.australiaeast.logic.azure.com:443/workflows/e28a90add06c495bbc9362b5109fb4c0/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=j2wEDAKOpnQ83iKkUa84aeGE6Nfb5604lp-z1-W4ek0",
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"dataFactoryName": "@pipeline().DataFactory",
						"pipelineName": "@pipeline().Pipeline",
						"messageHeader": "!!!FAILED!!!",
						"messageProcessed": "@variables('PipelineName')",
						"messageNotProcessed": "",
						"messageErrorDuringProcess": ""
					}
				}
			},
			{
				"name": "RunPipelineBudget",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "ProcessSourceBlob_budget_pl",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			},
			{
				"name": "SetVarBudget",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "RunPipelineBudget",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "PipelineName",
					"value": {
						"value": "@concat(activity('RunPipelineBudget').output,'\nFailed')",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"PipelineName": {
				"type": "String",
				"defaultValue": "MasterPipeline_pl"
			},
			"MasterUTCtag": {
				"type": "String",
				"defaultValue": "2019-11-07"
			}
		},
		"annotations": []
	}
}