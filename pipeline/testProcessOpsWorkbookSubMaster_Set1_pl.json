									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Budget_Treatment_Cost",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Budget Unit Cost ",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
