{
	"name": "Refresh_PBI",
	"properties": {
		"activities": [
			{
				"name": "Power_BI_Refresh_Trigger",
				"description": "Triggers a refresh of the dataset given in the parameter",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://prod-24.australiaeast.logic.azure.com:443/workflows/2ba27f8301324ad4886ec45cbbb4cf1c/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=9XZbRV5DBheXgF_055KtSIHXf2frxUUd7q3kNK83NKY",
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"Dataset": "@pipeline().parameters.Dataset"
					}
				}
			}
		],
		"parameters": {
			"Dataset": {
				"type": "string",
				"defaultValue": "Ops Summary"
			}
		},
		"annotations": []
	}
}